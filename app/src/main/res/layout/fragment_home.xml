<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.fragments.HomeFragment">

    <!-- TODO: Update blank fragment layout -->



        <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="@dimen/margin_normal"
            android:layout_marginStart="@dimen/margin_normal"
            android:layout_marginTop="@dimen/margin_big"
            android:layout_marginBottom="@dimen/margin_big"
            app:columnCount="2"
            app:rowCount="3"
            >
            <ImageButton
                android:id="@+id/assetsButton"
                android:contentDescription="@string/text_assets_menu"
                app:layout_column="0"
                app:layout_columnWeight="1"
                app:layout_row="0"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_add_business_24"
                >

            </ImageButton>
            <ImageButton
                android:id="@+id/documentsButton"
                android:contentDescription="@string/text_assets_menu"
                android:layout_marginStart="@dimen/margin_normal"
                app:layout_column="1"
                app:layout_columnWeight="1"
                app:layout_row="0"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_book_24" />
            <ImageButton
                android:id="@+id/input_assetButton"
                android:layout_marginTop="@dimen/margin_normal"
                android:contentDescription="@string/text_assets_menu"
                app:layout_column="0"
                app:layout_columnWeight="1"
                app:layout_row="1"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_add_circle_24" />
            <ImageButton
                android:id="@+id/output_assetButton"
                android:layout_marginTop="@dimen/margin_normal"
                android:contentDescription="@string/text_assets_menu"
                android:layout_marginStart="@dimen/margin_normal"
                app:layout_column="1"
                app:layout_columnWeight="1"
                app:layout_row="1"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_indeterminate_check_box_24" />
            <ImageButton
                android:id="@+id/my_suppliersButton"
                android:layout_marginTop="@dimen/margin_normal"
                android:contentDescription="@string/text_assets_menu"
                app:layout_column="0"
                app:layout_columnWeight="1"
                app:layout_row="2"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_supervised_user_circle_24" />
            <ImageButton
                android:id="@+id/my_clientsButton"
                android:layout_marginTop="@dimen/margin_normal"
                android:contentDescription="@string/text_assets_menu"
                android:layout_marginStart="@dimen/margin_normal"
                app:layout_column="1"
                app:layout_columnWeight="1"
                app:layout_row="2"
                app:layout_rowWeight="1"
                android:background="@color/md_blue_700"
                android:src="@drawable/ic_baseline_attach_money_24" />



        </androidx.gridlayout.widget.GridLayout>

<TextView
    android:id="@+id/text_view_prueba"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    android:text=""/>



</androidx.constraintlayout.widget.ConstraintLayout>